{
	"name": "@ctx-core/auth0-spa-svelte",
	"version": "1.1.0",
	"description": "ctx-core auth0 ui component & express endpoints",
	"keywords": [
		"ctx-core",
		"auth0",
		"svelte"
	],
	"homepage": "https://github.com/ctx-core/auth0-ui#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/auth0-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/auth0-spa-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"svelte": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/auth0": "workspace:^35.2.1",
		"@ctx-core/auth0-ui": "^15.0.0",
		"@ctx-core/dialog-ui-svelte": "^1.0.47",
		"@ctx-core/dom": "^11.2.33",
		"@ctx-core/function": "^21.6.1",
		"@ctx-core/nanostores": "workspace:^2.2.22",
		"@ctx-core/object": "^24.1.4",
		"auth0-js": "^9.20.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.61",
		"@swc/core": "^1.3.34",
		"@types/auth0-js": "^9.14.7",
		"rimraf": "^4.1.2",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.1",
		"tslib": "^2.5.0",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}
